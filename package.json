	"type": "module",
	"dependencies": {
		"@steeze-ui/heroicons": "^2.2.2",
		"@steeze-ui/svelte-icon": "^1.3.2",
		"daisyui": "^2.50.0",
		"svelte-inview": "^4.0.0"
	}
