		"svelte-sitemap": "^2.6.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.8"
	},
	"type": "module",
	"dependencies": {
		"@steeze-ui/heroicons": "^2.3.0",
		"@steeze-ui/svelte-icon": "^1.5.0",
		"daisyui": "^2.52.0",
		"eslint-plugin-svelte": "^2.36.0",
		"svelte-inview": "^4.0.2",
		"svelte-preprocess": "^5.1.3"
