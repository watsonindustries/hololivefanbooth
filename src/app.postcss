/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

@layer utilities {
	.animation-delay-1000 {
		animation-delay: 1s
	}
	.animation-delay-2000 {
		animation-delay: 2s
	}
	.animation-delay-3000 {
		animation-delay: 3s
	}
	.animation-delay-4000 {
		animation-delay: 4s
	}
}